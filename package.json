{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.3",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.3",
    "@types/node": "17.0.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.24.0",
    "prettier": "2.7.1",
    "turbo": "latest",
    "typescript": "4.8.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "16.17.1"
  }
}
