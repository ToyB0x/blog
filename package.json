{
	"name": "blog",
	"type": "module",
	"version": "1.0.0",
	"scripts": {
		"dev": "astro check --watch & astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,svelte,astro}\"",
		"format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,svelte,astro}\"",
		"lint": "eslint \"src/**/*.{js,ts,jsx,tsx,svelte,astro}\"",
		"lint:fix": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,svelte,astro}\"",
		"typecheck": "tsc --noEmit",
		"test:e2e": "start-server-and-test 'pnpm start' localhost:4321 'playwright test'"
	},
	"dependencies": {
		"@astrojs/check": "0.9.3",
		"@astrojs/mdx": "3.1.7",
		"@astrojs/rss": "4.0.7",
		"@astrojs/sitemap": "3.1.6",
		"@astrojs/tailwind": "5.1.1",
		"@astrojs/vercel": "7.8.1",
		"@vercel/analytics": "1.3.1",
		"astro": "4.15.10",
		"astro-expressive-code": "0.37.0",
		"astro-icon": "1.1.1",
		"clsx": "2.1.1",
		"mdast-util-to-string": "4.0.0",
		"reading-time": "1.5.0",
		"rehype-external-links": "3.0.0",
		"remark-unwrap-images": "4.0.0",
		"sharp": "0.33.5",
		"tailwind-merge": "2.5.2",
		"tailwindcss": "3.4.13",
		"typescript": "5.6.2"
	},
	"devDependencies": {
		"@playwright/test": "1.47.2",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/typography": "0.5.15",
		"@types/node": "22.7.4",
		"@typescript-eslint/parser": "8.8.0",
		"eslint": "9.11.1",
		"eslint-plugin-astro": "1.2.4",
		"eslint-plugin-jsx-a11y": "6.10.0",
		"prettier": "3.3.3",
		"prettier-config-standard": "7.0.0",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "0.6.8",
		"start-server-and-test": "2.0.8"
	},
	"engines": {
		"node": "20.17.0"
	},
	"packageManager": "pnpm@9.11.0",
	"volta": {
		"node": "20.17.0",
		"pnpm": "9.11.0"
	}
}
